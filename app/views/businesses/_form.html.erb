<%= form_for @business, :validate => true do |f| %>
  

  <div class="field">
    <%= f.label :business_name %>
    <%= f.text_field :business_name %>
  </div>
  <div class="field">
    <%= f.label :website %>
    <%= f.text_field :website %>
  </div>
<div class="field">
    <%= f.label :offers_history %>
    <%= f.text_field :offers_history %>
  </div>
<div class="field">
    <%= f.label :invoices %>
    <%= f.text_field :invoices %>
  </div>
<div class="field">
    <%= f.label :account_data %>
    <%= f.text_field :account_data %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
